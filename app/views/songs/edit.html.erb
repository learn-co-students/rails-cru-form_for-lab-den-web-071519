<h1>Update Song</h1>

<%= form_for @song do |f|%>
  <%= f.label :name, "Name"%><br>
  <%= f.text_field :name%><br>

  <%= f.label :artist_id, "Artist Id"%><br>
  <%= f.text_field :artist_id%><br>

  <%= f.label :genre_id, "Genre Id"%><br>
  <%= f.text_field :genre_id%><br>

  <%= f.submit "Update Song"%><br>

<% end %>
